    <!-- Mandiri bank transfer Instruction-->
    <string name="instruction">Instructions</string>
    <string name="mandiri_instruction_step1">1.\tEnter Your PIN.</string>
    <string name="mandiri_instruction_step2">2.\tOn the main menu, Choose \'others\'.</string>
    <string name="mandiri_instruction_step3">3.\tChoose Transfer.</string>
    <string name="mandiri_instruction_step4">4.\tChoose \'Online Transfer\'.</string>
    <string name="mandiri_instruction_step5">5.\tEnter the number 013 (code 013 and 16-digit account number).</string>
    <string name="mandiri_instruction_step6">6.\tThe reference number can be left blank and then choose correct.</string>


    <!-- bca/ prima bank transfer Instruction-->
    <string name="bca_instruction_step1">1.\tEnter Your PIN.</string>
    <string name="bca_instruction_step2">2.\tOn the main menu, Choose \'Other Transaction\'.</string>
    <string name="bca_instruction_step3">3.\tChoose \'Transfer\'.</string>
    <string name="bca_instruction_step4">4.\tChoose \'Other Bank Account\'.</string>
    <string name="bca_instruction_step5">5.\tEnter the number 013 (Bank Permata) and choose \'Correct\'.</string>
    <string name="bca_instruction_step6">6.\tEnter full amount to be paid (if the amount entered is not the same as the invoiced amount, the transaction will be declined).</string>
    <string name="bca_instruction_step7">7.\tEnter 16 digits payment Account No. and press \'Correct\'.</string>


    <!-- cimb transfer Instructions -->
    <string name="cimb_instruction_step1">1.\tPlease make sure that you have a User ID for CIMB Clicks and have registered your mPIN before going through with the payment.</string>
    <string name="cimb_instruction_step2">2.\tTransaction will be cancelled if payment is not completed within 2 hours.</string>
    <string name="cimb_instruction_step3">3.\tPlease make sure that there is no pop-up blocker on your browser.</string>


    <!-- Mandiri E Cash transfer Instruction-->
    <string name="mandiri__e_cash_instruction_step1">1.\tMake sure you have registered your mobile phone number for Mandiri e-cash.</string>
    <string name="mandiri__e_cash_instruction_step2">2.\tClick \'Confirm Payment\'.</string>
    <string name="mandiri__e_cash_instruction_step3">3.\tEnter your phone number and PIN.</string>
    <string name="mandiri__e_cash_instruction_step4">4.\tPlease wait for an SMS from Mandiri e-cash that contains your One Time Password (OTP).</string>
    <string name="mandiri__e_cash_instruction_step5">5.\tEnter the 6 digit OTP, then click
        \'pay\' to make payment.</string>
    <string name="mandiri__e_cash_instruction_step6">6.\tIf you haven\'t received the OTP or the
        OTP has expired, click \'cancel\' and retry your transaction.</string>


    <!-- Indomaret Instruction-->
    <string name="indomaret_instruction_step1">1.\tAfter confirming your payment, we will issue you a unique Payment Code number.</string>
    <string name="indomaret_instruction_step2">2.\tNote down your Payment Code and total amount.
        Don\'t worry, we will also mail you a copy of the payment instructions.</string>
    <string name="indomaret_instruction_step3">3.\tGo to an Indomaret store near you and provide the cashier with the Payment Code number.</string>
    <string name="indomaret_instruction_step4">4.\tThe cashier will then confirm the transaction by asking for the transaction amount and the merchant name.</string>
    <string name="indomaret_instruction_step5">5.\tConfirm the payment with the cashier.</string>
    <string name="indomaret_instruction_step6">6.\tYour transaction is successful! You should be receiving an email confirming your payment. Please keep your Indomaret payment receipt just in case you need help via support.</string>


    <!-- BBM Money Instruction-->
    <string name="bbm_money_instruction_step1">1.\tPlease click \'Confirm Payment\'.</string>
    <string name="bbm_money_instruction_step2">2.\tOpen or install the BBM™ Money app on your
        smartphone.</string>
    <string name="bbm_money_instruction_step3">3.\tMake sure you have enough balance in your BBM™ Money wallet.</string>
    <string name="bbm_money_instruction_step4">4.\tLogin to your BBM™ Money account and select
        \'Send To Company\'.</string>
    <string name="bbm_money_instruction_step5">5.\tSelect \'Online Shopping\' as the biller and
        then enter Payment Code into the \'Payment Code\' field.</string>
    <string name="bbm_money_instruction_step6">6.\tInput amount to be paid into \'Amount\'.</string>

