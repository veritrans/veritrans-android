    <!-- Mandiri bank transfer Instruction-->
    <string name="instruction">Instructions</string>

    <string name="mandiri_instruction_step1">1.\tOn the main menu, Choose \'Pay/Buy\'.</string>
    <string name="mandiri_instruction_step2">2.\tChoose \'Others\'.</string>
    <string name="mandiri_instruction_step3">3.\tChoose \'Multi Payment\'.</string>
    <string name="mandiri_instruction_step4">4.\tEnter 70012 (Veritrans company code) and press \'Correct\'.</string>
    <string name="mandiri_instruction_step5">5.\tEnter your Payment code and press \'Correct\'.</string>
    <string name="mandiri_instruction_step6">6.\tYour payment details will appear on the payment confirmation page. If the information is correct press \'Yes\'.</string>

    <string name="mandiri_internet_instruction_step1">1.\tLogin to Mandiri Internet Banking (ib.bankmandiri.co.id).</string>
    <string name="mandiri_internet_instruction_step2">2.\tFrom the main menu choose \'Payment\', then choose \'Multi Payment\'.</string>
    <string name="mandiri_internet_instruction_step3">3.\tSelect your account in \'From Account\', then \'Billing Name\' select \'Veritrans\'.</string>
    <string name="mandiri_internet_instruction_step4">4.\tEnter the \'Payment Code\' and you will receive your payment details.</string>
    <string name="mandiri_internet_instruction_step5">5.\tConfirm your payment using Mandiri Token.</string>


    <!-- bca/ prima bank transfer Instruction-->
    <!--BCA Transfer-->
    <string name="bca_instruction_step1">1.\tOn the main menu, Choose \'Other Transaction\'.</string>
    <string name="bca_instruction_step2">2.\tChoose \'Transfer\'.</string>
    <string name="bca_instruction_step3">3.\tChoose \'To BCA Virtual Account\'.</string>
    <string name="bca_instruction_step4">4.\tEnter your payment code (11 digit code) and press \'Correct\'.</string>
    <string name="bca_instruction_step5">5.\tEnter full amount to be paid and press \'Correct\'.</string>
    <string name="bca_instruction_step6">6.\tYour payment details will appear on the confirmation page. If information is correct press \'Yes\'.</string>
    <!--Klik BCA-->
    <string name="bca_klik_instruction_step1">1.\tChoose menu \'Fund Transfer\'.</string>
    <string name="bca_klik_instruction_step2">2.\tChoose \'Transfer to BCA Virtual Account\'.</string>
    <string name="bca_klik_instruction_step3">3.\t\'Input BCA Virtual Account Number\' or \'Choose from Transfer List\' and click \'Continue\'.</string>
    <string name="bca_klik_instruction_step4">4.\tAmount to be paid, account number and Merchant name will appear on the payment confirmation page, if information is right click \'Continue\'.</string>
    <string name="bca_klik_instruction_step5">5.\tGet your \'BCA token\' and input KEYBCA Response \'APPLI 1\' and click \'Submit\'.</string>
    <string name="bca_klik_instruction_step6">6.\tYour transaction is Done.</string>

    <!--KLIK BCA PAYMENT-->
    <string name="bca_klik_pay_instruction_step1">1.\tYou will be redirected to BCA KlikPay once you tap \'Confirm Payment\' button.</string>
    <string name="bca_klik_pay_instruction_step2">2.\tFor payment, you can choose to use KlikBCA Individu Account or BCA card.</string>
    <string name="bca_klik_pay_instruction_step3">3.\tFor more information about BCA KlikPay please contact Halo BCA at (021) 500888 or visit www.bca.co.id.</string>

    <!--m-BCA-->
    <string name="bca_mbanking_instruction_step1">1.\tChoose \'m-transfer\'.</string>
    <string name="bca_mbanking_instruction_step2">2.\tChoose \'Transfer\'</string>
    <string name="bca_mbanking_instruction_step3">3.\tChoose \'BCA Virtual Account\'.</string>
    <string name="bca_mbanking_instruction_step4">4.\tChoose \'account number\' that you want to use for payment.</string>
    <string name="bca_mbanking_instruction_step5">5.\tInput BCA Virtual Account Number and click \'OK\'.</string>
    <string name="bca_mbanking_instruction_step6">6.\tBCA Virtual Account Number and your account information will appear on the payment confirmation page and then click \'Send\'.</string>
    <string name="bca_mbanking_instruction_step7">7.\tClick \'OK\' on the payment confirmation page.</string>
    <string name="bca_mbanking_instruction_step8">8.\tEnter amount to be transferred then click \'OK\'.</string>
    <string name="bca_mbanking_instruction_step9">9.\tYour transaction is done.</string>
    <!-- cimb transfer Instructions -->
    <string name="cimb_instruction_step1">1.\tPlease make sure that you have a User ID for CIMB Clicks and have registered your mPIN before going through with the payment.</string>
    <string name="cimb_instruction_step2">2.\tTransaction will be cancelled if payment is not completed within 2 hours.</string>
    <string name="cimb_instruction_step3">3.\tPlease make sure that there is no pop-up blocker on your browser.</string>


    <!-- Mandiri E Cash transfer Instruction-->
    <string name="mandiri__e_cash_instruction_step1">1.\tMake sure you have registered your mobile phone number for Mandiri e-cash.</string>
    <string name="mandiri__e_cash_instruction_step2">2.\tClick \'Confirm Payment\'.</string>
    <string name="mandiri__e_cash_instruction_step3">3.\tEnter your phone number and PIN.</string>
    <string name="mandiri__e_cash_instruction_step4">4.\tPlease wait for an SMS from Mandiri e-cash that contains your One Time Password (OTP).</string>
    <string name="mandiri__e_cash_instruction_step5">5.\tEnter the 6 digit OTP, then click
        \'pay\' to make payment.</string>
    <string name="mandiri__e_cash_instruction_step6">6.\tIf you haven\'t received the OTP or the
        OTP has expired, click \'cancel\' and retry your transaction.</string>


    <!-- Indomaret Instruction-->
    <string name="indomaret_instruction_step1">1.\tAfter confirming your payment, we will issue you a unique Payment Code number.</string>
    <string name="indomaret_instruction_step2">2.\tNote down your Payment Code and total amount.
        Don\'t worry, we will also mail you a copy of the payment instructions.</string>
    <string name="indomaret_instruction_step3">3.\tGo to an Indomaret store near you and provide the cashier with the Payment Code number.</string>
    <string name="indomaret_instruction_step4">4.\tThe cashier will then confirm the transaction by asking for the transaction amount and the merchant name.</string>
    <string name="indomaret_instruction_step5">5.\tConfirm the payment with the cashier.</string>
    <string name="indomaret_instruction_step6">6.\tYour transaction is successful! You should be receiving an email confirming your payment. Please keep your Indomaret payment receipt just in case you need help via support.</string>


    <!-- BBM Money Instruction-->
    <string name="bbm_money_instruction_step1">1.\tPlease click \'Confirm Payment\'.</string>
    <string name="bbm_money_instruction_step2">2.\tOpen or install the BBM™ Money app on your
        smartphone.</string>
    <string name="bbm_money_instruction_step3">3.\tMake sure you have enough balance in your BBM™ Money wallet.</string>
    <string name="bbm_money_instruction_step4">4.\tLogin to your BBM™ Money account and select
        \'Send To Company\'.</string>
    <string name="bbm_money_instruction_step5">5.\tSelect \'Online Shopping\' as the biller and
        then enter Payment Code into the \'Payment Code\' field.</string>
    <string name="bbm_money_instruction_step6">6.\tInput amount to be paid into \'Amount\'.</string>

