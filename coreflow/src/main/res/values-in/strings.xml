    <!-- Mandiri bank transfer Instruction-->
    <string name="instruction">Instruksi</string>

    <string name="mandiri_instruction_step1">1.\tPada main menu, Pilih \'Pay/Buy\'.</string>
    <string name="mandiri_instruction_step2">2.\tPilih \'Others\'.</string>
    <string name="mandiri_instruction_step3">3.\tPilih \'Multi Payment\'.</string>
    <string name="mandiri_instruction_step4">4.\tMasukan 70012 (Veritrans company code) dan tekan \'Correct\'.</string>
    <string name="mandiri_instruction_step5">5.\tMasukkan kode pembayaran Anda dan tekan \'Correct\'.</string>
    <string name="mandiri_instruction_step6">6.\tRincian pembayaran Anda akan tampil pada halaman konfirmasi. Jika informasi tersebut benar tekan \'Yes\'.</string>

    <string name="mandiri_internet_instruction_step1">1.\tLogin ke Internet Banking Mandiri (ib.bankmandiri.co.id).</string>
    <string name="mandiri_internet_instruction_step2">2.\tPada main menu, pilih \'Payment\', kemudian pilih \'Multi Payment\'.</string>
    <string name="mandiri_internet_instruction_step3">3.\tPilih rekening Anda di \'From Account\', kemudian \'Billing Name\' pilih \'Veritrans\'.</string>
    <string name="mandiri_internet_instruction_step4">4.\tMasukkan \'Payment Code\' dan Anda akan menerima rincian pembayaran.</string>
    <string name="mandiri_internet_instruction_step5">5.\tKonfirmasi pembayaran melalui Mandiri Token.</string>

    <!-- bca/ prima bank transfer Instruction-->
    <!--BCA Transfer-->
    <string name="bca_instruction_step1">1.\tPada menu utama, pilih Transaksi lainnya.</string>
    <string name="bca_instruction_step2">2.\tPilih \'Transfer\'.</string>
    <string name="bca_instruction_step3">3.\tPilih \'Ke Rek BCA Virtual Account\'.</string>
    <string name="bca_instruction_step4">4.\tMasukkan nomor (11 digit No. Rekening pembayaran) lalu tekan \'Benar\'.</string>
    <string name="bca_instruction_step5">5.\tMasukkan jumlah tagihan yang akan anda bayar.</string>
    <string name="bca_instruction_step6">6.\tPada halaman konfirmasi transfer akan muncul detail pembayaran anda. Jika informasi telah sesuai tekan \'Ya\'.</string>

    <!--Klik BCA-->
    <string name="bca_klik_instruction_step1">1.\Pilih menu \'Fund Transfer\'.</string>
    <string name="bca_klik_instruction_step2">2.\tPilih \'Transfer to BCA Virtual Account\'.</string>
    <string name="bca_klik_instruction_step3">3.\t\'Masukkan Nomor Virtual Account BCA\' atau \'Pilih dari daftar transfer\' dan klik \'Continue\'.</string>
    <string name="bca_klik_instruction_step4">4.\tJumlah yang harus dibayarkan, nomor rekening dan nama merchant akan tampil pada halaman konfirmasi pembayaran, jika informasi tersebut benar, klik \'Continue\'.</string>
    <string name="bca_klik_instruction_step5">5.\tDapatkan \'BCA token\' dan masukkan KEYBCA Response \'APPLI 1\' dan klik \'Submit\'.</string>
    <string name="bca_klik_instruction_step6">6.\tTransaksi Anda selesai.</string>

    <!--KLIK BCA PAYMENT-->
    <string name="bca_klik_pay_instruction_step1">1.\tAnda akan dialihkan ke halaman BCA KlikPay setelah menekan tombol \'Confirm Payment\' .</string>
    <string name="bca_klik_pay_instruction_step2">2.\tUntuk pembayaran, Anda dapat memilih Rekening KlikBCA Individu atau kartu BCA.</string>
    <string name="bca_klik_pay_instruction_step3">3.\tUntuk informasi lebih lanjut mengenai BCA KlikPay harap kontak Halo BCA di (021) 500888 atau kunjungi www.bca.co.id.</string>

    <!--m-BCA-->
    <string name="bca_mbanking_instruction_step1">1.\tPilih \'m-transfer\'.</string>
    <string name="bca_mbanking_instruction_step2">2.\tPilih \'Transfer\'</string>
    <string name="bca_mbanking_instruction_step3">3.\tPilih \'BCA Virtual Account\'.</string>
    <string name="bca_mbanking_instruction_step4">4.\tPilih \'account number\' yang ingin anda gunakan untuk pembayaran.</string>
    <string name="bca_mbanking_instruction_step5">5.\tMasukkan Nomor Virtual Account BCA dan klik \'OK\'.</string>
    <string name="bca_mbanking_instruction_step6">6.\tInformasi nomor Virtual Account dan rekening anda information akan tampil pada halaman konfirmasi pembayaran dan kemudian klik \'Send\'.</string>
    <string name="bca_mbanking_instruction_step7">7.\tKlik \'OK\' pada halaman konfirmasi pembayaran.</string>
    <string name="bca_mbanking_instruction_step8">8.\tMasukkan jumlah yang ingin ditransfer kemudian klik \'OK\'.</string>
    <string name="bca_mbanking_instruction_step9">9.\tTransaksi Anda telah selesai</string>

    <!-- cimb transfer Instructions -->
    <string name="cimb_instruction_step1">1.\tPastikan bahwa Anda memiliki User ID untuk CIMB Clicks dan telah mendaftarkan mPIN Anda sebelum melanjutkan pembayaran.</string>
    <string name="cimb_instruction_step2">2.\tTransaksi akan dibatalkan apabila pembayaran tidak diselesaikan dalam 2 jam.</string>
    <string name="cimb_instruction_step3">3.\tPastikan bahwa tidak ada pop-up blocker pada browser Anda.</string>

    <!-- Mandiri E Cash transfer Instruction-->
    <string name="mandiri__e_cash_instruction_step1">1.\tPastikan no. HP anda telah terdaftar untuk Mandiri e-cash.</string>
    <string name="mandiri__e_cash_instruction_step2">2.\tKlik \'Confirm Payment\'.</string>
    <string name="mandiri__e_cash_instruction_step3">3.\tMasukkan no. HP dan PIN Anda.</string>
    <string name="mandiri__e_cash_instruction_step4">4.\tHarap tunggu sampai Anda menerima SMS dari Mandiri e-cash berisi One Time Password (OTP).</string>
    <string name="mandiri__e_cash_instruction_step5">5.\tMasukkan 6 digit OTP, lalu klik
        \'pay\' untuk melakukan pembayaran.</string>
    <string name="mandiri__e_cash_instruction_step6">6.\tJika Anda belum menerima OTP atau jika
        OTP sudah kadaluarsa, klik \'cancel\' dan ulangi transaksi.</string>

    <!-- Indomaret Instruction-->
    <string name="indomaret_instruction_step1">1.\tSetelah pembayaran dikonfirmasi, kami akan mengirimkan nomor Payment Code yang unik.</string>
    <string name="indomaret_instruction_step2">2.\tCatat Payment Code and jumlah total.
       Jangan khawatir, kami juga akan mengirimkan instruksi pembayaran melalui email.</string>
    <string name="indomaret_instruction_step3">3.\tKunjungi Indomaret terdekat dan berikan nomor Payment Code kepada kasir.</string>
    <string name="indomaret_instruction_step4">4.\tKasir akan melakukan konfirmasi transaksi dengan menanyakan jumlah transaksi dan nama merchant.</string>
    <string name="indomaret_instruction_step5">5.\tKonfirmasi Pembayaran melalui kasir.</string>
    <string name="indomaret_instruction_step6">6.\tTransaksi Anda berhasil! Anda akan menerima email konfirmasi pembayaran. Harap simpan struk pembayaran Indomaret Anda apabila anda memerlukan bantuan via support.</string>

    <!-- Permata/Alto bank transfer Instruction-->
    <string name="permata_instruction_step1">1.\tPada main menu, pilih \'Other Transaction\'.</string>
    <string name="permata_instruction_step2">2.\tPilih \'Payment\'.</string>
    <string name="permata_instruction_step3">3.\tPilih \'Other Payment\'.</string>
    <string name="permata_instruction_step4">4.\tPilih \'Virtual Account\'.</string>
    <string name="permata_instruction_step5">5.\tMasukkan 16 digits rekening pembayaran. lalu klik \'Correct\'.</string>
    <string name="permata_instruction_step6">6.\tJumlah yang perlu dibayarkan, no. rekening, dan nama merchant akan tampil pada halaman konfirmasi. Jika informasi tersebut benar, klik \'Correct\'.</string>
    <string name="permata_instruction_step7">7.\tPilih rekening pembayaran Anda dan klik \'Correct\'.</string>
    <string name="bank_transfer_dummy_text">* Bergantung pada biaya bank/transfer dan transaksi minimum sebesar Rp50,000, - untuk bank mandiri</string>

    <string name="bri_instruction_step1">1.Pastikan bahwa Anda memiliki User ID untuk BRI e-Pay dan telah terdaftar untuk mTOKEN sebelum melanjutkan pembayaran </string>
    <string name="bri_instruction_step2">2.Untuk mendapatkan User ID untuk BRI e-Pay, harap mendaftar pada ATM BRI terdekat
    </string>
    <string name="bri_instruction_step3">3.Harap kunjungi kantor cabang BRI terdekat untuk mendaftar mTOKEN.
    </string>
    <string name="bri_instruction_step4">4.Pembayaran melalui BRI e-Pay akan diproses secara online dan saldo rekening BRI akan dikurangi secara otomatis berdasarkan total harga.
    </string>
    <string name="bri_instruction_step5">5.Transaksi akan dibatalkan apabila pembayaran tidak dilakukan dalam 2 jam.
    </string>

