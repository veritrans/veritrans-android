    <!-- Mandiri bank transfer Instruction-->
    <string name="instruction">Instruksi</string>

    <string name="mandiri_instruction_step1">Pada menu utama, Pilih <b>Bayar/Beli</b>.</string>
    <string name="mandiri_instruction_step2">Pilih <b>Lainnya</b>.</string>
    <string name="mandiri_instruction_step3">Pilih <b>Multi Payment</b>.</string>
    <string name="mandiri_instruction_step4">Masukan 70012 (kode perusahaan Midtrans) lalu tekan <b>Benar</b>.</string>
    <string name="mandiri_instruction_step5">Masukkan <b>Kode Pembayaran</b> Anda lalu tekan <b>Benar</b>.</string>
    <string name="mandiri_instruction_step6"><b>Detail pembayaran Anda</b> akan muncul pada halaman konfirmasi. Jika informasi telah sesuai tekan <b>Ya</b>.</string>

    <string name="mandiri_internet_instruction_step1">Login ke Internet Banking Mandiri (<a href="http://ib.bankmandiri.co.id/">http://ib.bankmandiri.co.id/</a>).</string>
    <string name="mandiri_internet_instruction_step2">Pada menu utama, pilih <b>Bayar</b>, kemudian pilih <b>Multi Payment</b>.</string>
    <string name="mandiri_internet_instruction_step3">Pilih akun Anda di <b>Dari Rekening</b>, kemudian di Penyedia Jasa pilih <b>Midtrans</b>.</string>
    <string name="mandiri_internet_instruction_step4">Masukkan <b>Kode Pembayaran</b> Anda dan klik <b>Lanjutkan</b>.</string>
    <string name="mandiri_internet_instruction_step5">Konfirmasi pembayaran Anda menggunakan Mandiri Token.</string>

    <!-- bca/ prima bank transfer Instruction-->
    <!--BCA Transfer-->
    <string name="bca_instruction_step1">Pada menu utama, pilih <b>Transaksi Lainnya</b>.</string>
    <string name="bca_instruction_step2">Pilih <b>Transfer</b>.</string>
    <string name="bca_instruction_step3">Pilih <b>Ke Rek BCA Virtual Account</b>.</string>
    <string name="bca_instruction_step4">Masukkan Nomor Rekening pembayaran anda(11 digit) lalu tekan <b>Benar</b>.</string>
    <string name="bca_instruction_step5">Masukkan <b>Jumlah Tagihan</b> yang akan Anda bayar.</string>
    <string name="bca_instruction_step6"><b>Detail pembayaran Anda</b> akan muncul pada halaman konfirmasi. jika informasi telah sesuai tekan <b>Ya</b>.</string>

    <!--Klik BCA-->
    <string name="bca_klik_instruction_step1">Pilih menu <b>Transfer Dana</b>.</string>
    <string name="bca_klik_instruction_step2">Pilih <b>Transfer ke BCA Virtual Account</b>.</string>
    <string name="bca_klik_instruction_step3"><b>Masukkan nomor BCA Virtual Account</b>, atau <b>Pilih Dari Daftar Transfer</b>.</string>
    <string name="bca_klik_instruction_step4">Jumlah yang harus dibayarkan, nomor rekening dan nama merchant akan tampil pada halaman konfirmasi pembayaran, jika informasi benar, klik <b>Lanjutkan</b>.</string>
    <string name="bca_klik_instruction_step5">Ambil <b>BCA token</b> Anda dan masukkan KEYBCA Response <b>APPLI 1</b> dan klik <b>Submit</b>.</string>
    <string name="bca_klik_instruction_step6">Transaksi Anda selesai.</string>

    <!--BCA KlikPay -->
    <string name="bca_klik_pay_instruction_step1">Anda akan diarahkan ke halaman BCA KlikPay setelah menekan tombol <b>Konfirmasi Pembayaran</b>.</string>
    <string name="bca_klik_pay_instruction_step2">Untuk pembayaran, Anda dapat memilih akun KlikBCA Individu atau kartu kredit BCA.</string>
    <string name="bca_klik_pay_instruction_step3">Untuk informasi lebih lanjut mengenai BCA KlikPay harap kontak Halo BCA di (021) 1500888 atau kunjungi <a href="http://klikbca.com/KlikPay/klikpay.html">http://klikbca.com/KlikPay/klikpay.html</a>.</string>

    <!--m-BCA-->
    <string name="bca_mbanking_instruction_step1">Pilih <b>m-Transfer</b>.</string>
    <string name="bca_mbanking_instruction_step2">Pilih <b>Transfer</b>.</string>
    <string name="bca_mbanking_instruction_step3">Pilih <b>BCA Virtual Account</b>.</string>
    <string name="bca_mbanking_instruction_step4">Pilih <b>nomor rekening</b> yang akan digunakan untuk pembayaran.</string>
    <string name="bca_mbanking_instruction_step5">Masukkan nomor BCA Virtual Account, lalu pilih <b>Ok</b>.</string>
    <string name="bca_mbanking_instruction_step6">Nomor BCA Virtual Account dan nomor rekening Anda akan terlihat pada halaman konfirmasi rekening, kemudian pilih <b>Kirim</b>.</string>
    <string name="bca_mbanking_instruction_step7">Pilih <b>Ok</b> pada halaman konfirmasi pembayaran.</string>
    <string name="bca_mbanking_instruction_step8">Masukkan jumlah nominal yang akan ditransfer dan kemudian pilih <b>Ok</b>.</string>
    <string name="bca_mbanking_instruction_step9">Transaksi Anda telah selesai</string>

    <!-- cimb transfer Instructions -->
    <string name="cimb_instruction_step1">Pastikan bahwa Anda memiliki User ID untuk CIMB Clicks dan telah mendaftarkan mPIN Anda sebelum melanjutkan pembayaran.</string>
    <string name="cimb_instruction_step2">Transaksi akan dibatalkan apabila pembayaran tidak diselesaikan dalam 2 jam.</string>
    <string name="cimb_instruction_step3">Pastikan bahwa tidak ada pop-up blocker pada browser Anda.</string>

    <!-- Mandiri E Cash transfer Instruction-->
    <string name="mandiri__e_cash_instruction_step1">Pastikan no. HP anda telah terdaftar untuk LINE Pay e-cash / mandiri e-cash.</string>
    <string name="mandiri__e_cash_instruction_step2">Klik <b>Confirm Payment</b>.</string>
    <string name="mandiri__e_cash_instruction_step3">Masukkan <b>nomor ponsel</b> dan <b>PIN</b> Anda.</string>
    <string name="mandiri__e_cash_instruction_step4">Harap tunggu sampai Anda menerima SMS dari LINE Pay e-cash / mandiri e-cash berisi One Time Password (OTP).</string>
    <string name="mandiri__e_cash_instruction_step5"><b>Masukkan kode 6 angka OTP yang diterima</b>, kemudian tekan tombol
        <b>Bayar</b> untuk melakukan pembayaran.</string>
    <string name="mandiri__e_cash_instruction_step6">Jika Anda belum menerima OTP atau jika
        OTP sudah kadaluarsa, klik <b>Batal</b> dan ulangi transaksi.</string>

    <!-- Indomaret Instruction-->
    <string name="indomaret_instruction_step1">Setelah melakukan <b>Konfirmasi Pembayaran</b>, Anda akan diberikan Kode Pembayaran yang unik.</string>
    <string name="indomaret_instruction_step2">Salinlah Kode Pembayaran Anda dan jumlah pembayaran yang akan dibayarkan.
       Jangan khawatir, Kami juga akan mengirimkan salinan kode pembayaran dan instruksi pembayaran melalui email.</string>
    <string name="indomaret_instruction_step3">Kunjungi Indomaret terdekat dan berikan nomor Kode Pembayaran Anda ke kasir.</string>
    <string name="indomaret_instruction_step4">Kasir Indomaret akan mengkonfirmasi transaksi dengan menanyakan jumlah transaksi dan nama merchant.</string>
    <string name="indomaret_instruction_step5">Konfirmasi Pembayaran Anda ke kasir.</string>
    <string name="indomaret_instruction_step6">Transaksi Anda berhasil! Anda akan menerima email konfirmasi pembayaran. Harap simpan struk pembayaran Indomaret Anda, apabila Anda memerlukan bantuan via support.</string>

    <!-- Permata/Alto bank transfer Instruction-->
    <string name="permata_instruction_step1">Pada menu utama, pilih <b>Transaksi Lainnya</b>.</string>
    <string name="permata_instruction_step2">Pilih <b>Pembayaran</b>.</string>
    <string name="permata_instruction_step3">Pilih <b>Pembayaran Lainnya</b>.</string>
    <string name="permata_instruction_step4">Pilih <b>Virtual Account</b>.</string>
    <string name="permata_instruction_step5">Masukkan <b>16 digit No. Virtual Account</b> yang dituju, lalu tekan <b>Benar</b>.</string>
    <string name="permata_instruction_step6"><b>Detail pembayaran anda</b> akan muncul pada halaman konfirmasi. Jika informasi telah sesuai tekan <b>Benar</b>.</string>
    <string name="permata_instruction_step7">Pilih rekening pembayaran Anda dan tekan <b>Benar</b>.</string>
    <string name="bank_transfer_dummy_text">* Bergantung pada biaya bank/transfer dan transaksi minimum sebesar Rp50,000, - untuk bank mandiri</string>

    <string name="bri_instruction_step1">Pastikan bahwa Anda memiliki User ID untuk BRI e-Pay dan telah terdaftar untuk mTOKEN sebelum melanjutkan pembayaran.</string>
    <string name="bri_instruction_step2">Untuk mendapatkan User ID untuk BRI e-Pay, harap mendaftar pada ATM BRI terdekat</string>
    <string name="bri_instruction_step3">Harap kunjungi kantor cabang BRI terdekat untuk mendaftar mTOKEN.</string>
    <string name="bri_instruction_step4">Pembayaran melalui BRI e-Pay akan diproses secara online dan saldo rekening BRI akan dikurangi secara otomatis berdasarkan total harga.</string>
    <string name="bri_instruction_step5">Transaksi akan dibatalkan apabila pembayaran tidak dilakukan dalam 2 jam.</string>

    <!-- Kioson Instruction-->
    <string name="kioson_instruction_step1">Setelah melakukan <b>Konfirmasi Pembayaran</b>, Anda akan diberikan Kode Pembayaran yang unik.</string>
    <string name="kioson_instruction_step2">Salinlah Kode Pembayaran Anda dan jumlah pembayaran pembayaran yang akan dibayarkan.
       Jangan khawatir, Kami akan mengirimkan salinan Kode Pembayaran dan Instruksi Pembayaran melalui email.</string>
    <string name="kioson_instruction_step3">Kunjungi outlet Kioson terdekat dan berikan nomor Kode Pembayaran Anda ke kasir.</string>
    <string name="kioson_instruction_step4">Kasir akan membuka menu Kioson Cash Point, kemudian kasir akan memilih <b>Keagenan</b>.</string>
    <string name="kioson_instruction_step5">Pilih nama merchant.</string>
    <string name="kioson_instruction_step6">Pilih sub menu <b>Pembayaran</b>.</string>
    <string name="kioson_instruction_step7">Masukkan kode pembayaran anda dan dan detail pembayaran anda akan ditampilkan.</string>
    <string name="kioson_instruction_step8">Konfirmasi pembayaran  Anda dan cetak bukti pembayarannya.</string>
    <string name="kioson_instruction_step9">Transaksi Anda berhasil! Anda akan menerima email konfirmasi pembayaran. Harap simpan struk pembayaran Kioson Anda, apabila Anda memerlukan bantuan via support.</string>


