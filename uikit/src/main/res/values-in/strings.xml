    <!-- Mandiri bank transfer Instruction-->
    <string name="instruction">Instruksi</string>

    <string name="mandiri_instruction_step1">Pada main menu, Pilih \'Pay/Buy\'.</string>
    <string name="mandiri_instruction_step2">Pilih \'Others\'.</string>
    <string name="mandiri_instruction_step3">Pilih \'Multi Payment\'.</string>
    <string name="mandiri_instruction_step4">Masukan 70012 (Midtrans company code) dan tekan \'Correct\'.</string>
    <string name="mandiri_instruction_step5">Masukkan kode pembayaran Anda dan tekan \'Correct\'.</string>
    <string name="mandiri_instruction_step6">Rincian pembayaran Anda akan tampil pada halaman konfirmasi. Jika informasi tersebut benar tekan \'Yes\'.</string>

    <string name="mandiri_internet_instruction_step1">1.\tLogin ke Internet Banking Mandiri (<a href="http://ib.bankmandiri.co.id/">http://ib.bankmandiri.co.id/</a>).</string>
    <string name="mandiri_internet_instruction_step2">2.\tPada main menu, pilih \'Payment\', kemudian pilih \'Multi Payment\'.</string>
    <string name="mandiri_internet_instruction_step3">3.\tPilih rekening Anda di \'From Account\', kemudian \'Billing Name\' pilih \'Midtrans\'.</string>
    <string name="mandiri_internet_instruction_step4">4.\tMasukkan \'Payment Code\' dan Anda akan menerima rincian pembayaran.</string>
    <string name="mandiri_internet_instruction_step5">5.\tKonfirmasi pembayaran melalui Mandiri Token.</string>

    <!-- bca/ prima bank transfer Instruction-->
    <!--BCA Transfer-->
    <string name="bca_instruction_step1">Pada menu utama, pilih Transaksi lainnya.</string>
    <string name="bca_instruction_step2">Pilih \'Transfer\'.</string>
    <string name="bca_instruction_step3">Pilih \'Ke Rek BCA Virtual Account\'.</string>
    <string name="bca_instruction_step4">Masukkan nomor (11 digit No. Rekening pembayaran) lalu tekan \'Benar\'.</string>
    <string name="bca_instruction_step5">Masukkan jumlah tagihan yang akan anda bayar.</string>
    <string name="bca_instruction_step6">Pada halaman konfirmasi transfer akan muncul detail pembayaran anda. Jika informasi telah sesuai tekan \'Ya\'.</string>

    <!--Klik BCA-->
    <string name="bca_klik_instruction_step1">1.\tPilih menu \'Fund Transfer\'.</string>
    <string name="bca_klik_instruction_step2">2.\tPilih \'Transfer to BCA Virtual Account\'.</string>
    <string name="bca_klik_instruction_step3">3.\t\'Masukkan Nomor Virtual Account BCA\' atau \'Pilih dari daftar transfer\' dan klik \'Continue\'.</string>
    <string name="bca_klik_instruction_step4">4.\tJumlah yang harus dibayarkan, nomor rekening dan nama merchant akan tampil pada halaman konfirmasi pembayaran, jika informasi tersebut benar, klik \'Continue\'.</string>
    <string name="bca_klik_instruction_step5">5.\tDapatkan \'BCA token\' dan masukkan KEYBCA Response \'APPLI 1\' dan klik \'Submit\'.</string>
    <string name="bca_klik_instruction_step6">6.\tTransaksi Anda selesai.</string>

    <!--KLIK BCA PAYMENT-->
    <string name="bca_klik_pay_instruction_step1">Anda akan dialihkan ke halaman BCA KlikPay setelah menekan tombol \'Konfirmasi Pembayaran\' .</string>
    <string name="bca_klik_pay_instruction_step2">Untuk pembayaran, Anda dapat memilih akun KlikBCA Individu atau kartu kredit BCA.</string>
    <string name="bca_klik_pay_instruction_step3">Untuk informasi lebih lanjut mengenai BCA KlikPay harap kontak Halo BCA di (021) 1500888 atau kunjungi <a href="http://klikbca.com/KlikPay/klikpay.html">http://klikbca.com/KlikPay/klikpay.html</a>.</string>

    <!--m-BCA-->
    <string name="bca_mbanking_instruction_step1">1.\tPilih \'m-transfer\'.</string>
    <string name="bca_mbanking_instruction_step2">2.\tPilih \'Transfer\'</string>
    <string name="bca_mbanking_instruction_step3">3.\tPilih \'BCA Virtual Account\'.</string>
    <string name="bca_mbanking_instruction_step4">4.\tPilih \'account number\' yang ingin anda gunakan untuk pembayaran.</string>
    <string name="bca_mbanking_instruction_step5">5.\tMasukkan Nomor Virtual Account BCA dan klik \'OK\'.</string>
    <string name="bca_mbanking_instruction_step6">6.\tInformasi nomor Virtual Account dan rekening anda information akan tampil pada halaman konfirmasi pembayaran dan kemudian klik \'Send\'.</string>
    <string name="bca_mbanking_instruction_step7">7.\tKlik \'OK\' pada halaman konfirmasi pembayaran.</string>
    <string name="bca_mbanking_instruction_step8">8.\tMasukkan jumlah yang ingin ditransfer kemudian klik \'OK\'.</string>
    <string name="bca_mbanking_instruction_step9">9.\tTransaksi Anda telah selesai</string>

    <!-- cimb transfer Instructions -->
    <string name="cimb_instruction_step1">Pastikan bahwa Anda memiliki User ID untuk CIMB Clicks dan telah mendaftarkan mPIN Anda sebelum melanjutkan pembayaran.</string>
    <string name="cimb_instruction_step2">Transaksi akan dibatalkan apabila pembayaran tidak diselesaikan dalam 2 jam.</string>
    <string name="cimb_instruction_step3">Pastikan bahwa tidak ada pop-up blocker pada browser Anda.</string>

    <!-- Mandiri E Cash transfer Instruction-->
    <string name="mandiri__e_cash_instruction_step1">Pastikan no. HP anda telah terdaftar untuk Mandiri e-cash.</string>
    <string name="mandiri__e_cash_instruction_step2">Klik \'Confirm Payment\'.</string>
    <string name="mandiri__e_cash_instruction_step3">Masukkan no. HP dan PIN Anda.</string>
    <string name="mandiri__e_cash_instruction_step4">Harap tunggu sampai Anda menerima SMS dari Mandiri e-cash berisi One Time Password (OTP).</string>
    <string name="mandiri__e_cash_instruction_step5">Masukkan 6 digit OTP, lalu klik
        \'pay\' untuk melakukan pembayaran.</string>
    <string name="mandiri__e_cash_instruction_step6">Jika Anda belum menerima OTP atau jika
        OTP sudah kadaluarsa, klik \'cancel\' dan ulangi transaksi.</string>

    <!-- Indomaret Instruction-->
    <string name="indomaret_instruction_step1">Setelah pembayaran dikonfirmasi, kami akan mengirimkan nomor Payment Code yang unik.</string>
    <string name="indomaret_instruction_step2">Catat Payment Code and jumlah total.
       Jangan khawatir, kami juga akan mengirimkan instruksi pembayaran melalui email.</string>
    <string name="indomaret_instruction_step3">Kunjungi Indomaret terdekat dan berikan nomor Payment Code kepada kasir.</string>
    <string name="indomaret_instruction_step4">Kasir akan melakukan konfirmasi transaksi dengan menanyakan jumlah transaksi dan nama merchant.</string>
    <string name="indomaret_instruction_step5">Konfirmasi Pembayaran melalui kasir.</string>
    <string name="indomaret_instruction_step6">Transaksi Anda berhasil! Anda akan menerima email konfirmasi pembayaran. Harap simpan struk pembayaran Indomaret Anda apabila anda memerlukan bantuan via support.</string>

    <!-- Permata/Alto bank transfer Instruction-->
    <string name="permata_instruction_step1">Pada main menu, pilih \'Other Transaction\'.</string>
    <string name="permata_instruction_step2">Pilih \'Payment\'.</string>
    <string name="permata_instruction_step3">Pilih \'Other Payment\'.</string>
    <string name="permata_instruction_step4">Pilih \'Virtual Account\'.</string>
    <string name="permata_instruction_step5">Masukkan 16 digits rekening pembayaran. lalu klik \'Correct\'.</string>
    <string name="permata_instruction_step6">Jumlah yang perlu dibayarkan, no. rekening, dan nama merchant akan tampil pada halaman konfirmasi. Jika informasi tersebut benar, klik \'Correct\'.</string>
    <string name="permata_instruction_step7">Pilih rekening pembayaran Anda dan klik \'Correct\'.</string>
    <string name="bank_transfer_dummy_text">* Bergantung pada biaya bank/transfer dan transaksi minimum sebesar Rp50,000, - untuk bank mandiri</string>

    <string name="bri_instruction_step1">Pastikan bahwa Anda memiliki User ID untuk BRI e-Pay dan telah terdaftar untuk mTOKEN sebelum melanjutkan pembayaran </string>
    <string name="bri_instruction_step2">Untuk mendapatkan User ID untuk BRI e-Pay, harap mendaftar pada ATM BRI terdekat
    </string>
    <string name="bri_instruction_step3">Harap kunjungi kantor cabang BRI terdekat untuk mendaftar mTOKEN.
    </string>
    <string name="bri_instruction_step4">Pembayaran melalui BRI e-Pay akan diproses secara online dan saldo rekening BRI akan dikurangi secara otomatis berdasarkan total harga.
    </string>
    <string name="bri_instruction_step5">Transaksi akan dibatalkan apabila pembayaran tidak dilakukan dalam 2 jam.
    </string>

    <!-- Kioson Instruction-->
    <string name="kioson_instruction_step1">Setelah pembayaran dikonfirmasi, kami akan mengirimkan nomor Payment Code yang unik.</string>
    <string name="kioson_instruction_step2">Catat Payment Code and jumlah total.
       Jangan khawatir, kami juga akan mengirimkan instruksi pembayaran melalui email.</string>
    <string name="kioson_instruction_step3">Kunjungi outlet kioson terdekat dan berikan nomor Payment Code kepada kasir.</string>
    <string name="kioson_instruction_step4">Kasir akan membuka menu kioson Cash Point, kemudian kasir akan memilih "Keagenan".</string>
    <string name="kioson_instruction_step5">Pilih nama merchant.</string>
    <string name="kioson_instruction_step6">Pilih sub menu "Pembayaran".</string>
    <string name="kioson_instruction_step7">Masukkan kode pembayaran anda dan dan detail pembayaran anda akan ditampilkan.</string>
    <string name="kioson_instruction_step8">Konfirmasi pembayaran dan cetak bukti pembayarannya.</string>
    <string name="kioson_instruction_step9">Transaksi Anda berhasil! Anda akan menerima email konfirmasi pembayaran. Harap simpan struk pembayaran Anda apabila anda memerlukan bantuan via support.</string>


